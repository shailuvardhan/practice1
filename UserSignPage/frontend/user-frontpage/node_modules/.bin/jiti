#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/jiti@1.21.6/node_modules/jiti/bin/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/jiti@1.21.6/node_modules/jiti/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/jiti@1.21.6/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules:/home/theia/practice1/UserSignPage/frontend/node_modules:/home/theia/practice1/UserSignPage/node_modules:/home/theia/practice1/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/jiti/bin/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/jiti/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/jiti@1.21.6/node_modules/jiti/bin/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/jiti@1.21.6/node_modules/jiti/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/jiti@1.21.6/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/.pnpm/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules:/home/theia/practice1/UserSignPage/frontend/node_modules:/home/theia/practice1/UserSignPage/node_modules:/home/theia/practice1/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/jiti/bin/node_modules:/home/theia/practice1/UserSignPage/frontend/user-frontpage/node_modules/jiti/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../jiti/bin/jiti.js" "$@"
fi
